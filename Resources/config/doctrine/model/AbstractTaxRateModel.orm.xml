<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <mapped-superclass name="Miracode\StripeBundle\Model\AbstractTaxRateModel">
        <field name="active" column="active" type="boolean" />
        <field name="created" column="created" type="integer" />
        <field name="description" column="description" type="string" nullable="true"/>
        <field name="displayName" column="display_name" type="string"/>
        <field name="inclusive" column="inclusive" type="boolean" />
        <field name="jurisdiction" column="jurisdiction" type="string" nullable="true"/>
        <field name="livemode" column="livemode" type="boolean" />
        <field name="metadata" column="metadata" type="array" nullable="true" />
        <field name="percentage" column="percentage" type="integer" />
        <id name="id" column="id" type="string" length="180" />
    </mapped-superclass>

</doctrine-mapping>
