{
    "name": "aimir/stript-bundle",
    "description": "Integrates stripe in Symfony2",
    "keywords": ["symfony2", "stripe", "payment"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name" : "Aleksey Mirovskyi",
            "email": "mirovskyi@gmail.com"
        }
    ],
    "require": {
        "php"                          : ">=5.3.0",
        "symfony/dependency-injection" : "~2.3",
        "symfony/event-dispatcher"     : "~2.3",
        "symfony/config"               : "~2.3",
        "symfony/yaml"                 : "~2.3",
        "stripe/stripe-php"            : "~2.0"
    },
    "suggest": {
        "symfony/framework-bundle" : "To use this lib as a full Symfony Bundle and to use the profiler data collector"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Aimir\\StripeBundle\\": ""
        }
    }
}
